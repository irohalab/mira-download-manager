      "@types/uuid": "^8.3.1",
      "ava": "^4.2.0",
      "cross-env": "^7.0.3",
      "rimraf": "^3.0.2",
      "ts-node": "^10.7.0",
      "typescript": "^5.5.4"
   },
   "dependencies": {
      "@aws-sdk/client-s3": "^3.821.0",
      "@irohalab/mira-shared": "5.1.1",
      "@mikro-orm/core": "^6.5.2",
      "@mikro-orm/migrations": "^6.5.2",
      "@mikro-orm/postgresql": "^6.5.2",
      "@sentry/node": "^7.120.4",
      "amqplib": "^0.10.9",
      "axios": "1.13.6",
      "cors": "^2.8.5",
      "form-data": "^4.0.0",
      "inversify": "^6.0.3",
      "inversify-express-utils": "^6.5.0",
