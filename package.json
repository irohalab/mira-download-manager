      "uuid": "^8.3.2"
   },
   "scripts": {
      "start": "cross-env DEBUG=true ts-node src/main.ts",
      "start:server": "cross-env DEBUG=true ts-node src/WebServer.ts",
      "clean:dist": "rimraf dist/*",
      "build": "npm run clean:dist && tsc -p tsconfig.prod.json && cp package.json dist",
      "prod:main": "node dist/main.js",
      "prod:server": "node dist/WebServer.js",
      "migrate:upgrade": "ts-node src/migrate.ts --upgrade",
      "migrate:generate": "ts-node src/migrate.ts --generate",
      "migrate:init": "ts-node src/migrate.ts --init",
      "migrate:sync": "ts-node src/migrate.ts --sync",
      "migrate:sync:silent": "ts-node src/migrate.ts --sync --silent"
   }
